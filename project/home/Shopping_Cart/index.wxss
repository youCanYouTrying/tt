/* home/Shopping_Cart/index.wxss */
.shopping{
  width: 100%;
  height: 100%;
  background: #f5f5f5;
}
.shopping .heard{
  width: 100%;
  height: 35px;
  position: fixed;
  top: 0px;
  left: 0px;
  background: #fff;
  z-index: 9;

}
.shopping .heard .heard_num{
  float: left;
  font-size: 14px;
  line-height: 35px;
  padding-left: 15px;
  box-sizing: border-box;
}
.shopping .heard .heard_edit{
  float: right;
  font-size: 14px;
  line-height: 35px;
  padding-right: 15px;
  box-sizing: border-box;
}

/*滑动删除*/
.touch-item {
font-size: 14px;
display: flex;
justify-content: space-between;
width: 100%;
overflow: hidden
}
.content {
width: 100%;
padding: 10px;
line-height: 22px;
margin-right:0;
-webkit-transition: all 0.4s;
transition: all 0.4s;
-webkit-transform: translateX(90px);
transform: translateX(90px);
margin-left: -90px
}
.del {
background-color:#fe6e78;
width: 90px;
display: flex;
flex-direction: column;
align-items: center;
justify-content: center;
color: #fff;
-webkit-transform: translateX(90px);
transform: translateX(90px);
-webkit-transition: all 0.4s;
transition: all 0.4s;
}
