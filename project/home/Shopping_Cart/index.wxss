} */

/* .touch-move-active .content, */
.touch-move-active .del {
-webkit-transform: translateX(0);
transform: translateX(0);
}

/*滑动删除end*/
/*内容部分*/
.shopping .shopp_cen{
  width: 100%;
  /* margin-bottom: 40px; */
  padding-top: 40px;
  /* height: 100%; */
  overflow: auto;
}
.shopping .shopp_cen .shopp_items{
  width: 100%;
  padding: 10px;
  box-sizing: border-box;
  margin-bottom: 10px;
}
.shopp_items .shopp_item{
  width: 100%;
  margin-bottom: 15px;
   background: #fff;
   padding-bottom: 10px;
   padding-top: 10px;
   box-sizing: border-box;
   padding-left: 20rpx;
   box-shadow:2px 2px 1px #eee;
}
 .shopp_item .shop_title{
  box-sizing: border-box;
  width: 100%;
  display:flex;
  display:-webkit-flex;
  justify-content:flex-start;
  align-items:center;
  position: relative;
}
.shopp_item .shop_title .yu{
    position: absolute;
    top: -4px;
    display: inline-block;
    right: 5px;
    border: 1px solid #3c9df8;
    font-size: 12px;
    color: #3c9df8;
    padding: 3px 7px;
    box-sizing: border-box;
}
.lefts{
    width:40rpx;
    height: 40rpx;
    text-align:center;
    border:1rpx solid #d6d6d6;
    border-radius: 50%;
    box-sizing: border-box;
    position: relative
}
.lefts .icon_none{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 0;
  left: -1rpx;
  /* transform: translateY(50%) */

}
.shop_title .title_p{
  font-size: 15px;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 20rpx;
}
/*活动*/
.shopping .shopp_cen .shopp_items .shopp_item .activity{
  width: 100%;
  font-size: 24rpx;
  padding: 20rpx 0 0 0;
  box-sizing: border-box;
  color: #f7636d;
}
.shopping .shopp_cen .shopp_items .shopp_item .activity .activ_span{
  color: #f7636d;
  display: inline-block;
  padding: 0px 5px;
  box-sizing: border-box;
  border: 1px solid #f7636d;
  margin-right: 10px;
}
.shopping .shopp_cen .shopp_items .shopp_item .items_cen{
  width: 100%;
  border-bottom: 1px solid #f0f0f0;
  margin-top: 20rpx;
}
.shopping .shopp_cen .shopp_items .shopp_item .items_cen .cenitem{
     width: 100%;
  display:flex;
  display:-webkit-flex;
  justify-content:flex-start;
  align-items:center;
  margin: 8px 0px;
}
.shopping .shopp_cen .shopp_items .shopp_item .items_cen .cenitem  .lefts{
    width:40rpx;
    height: 40rpx;
    text-align:center;
    border:1rpx solid #d6d6d6;
    border-radius: 50%;
    box-sizing: border-box;
} 
.shopping .shopp_cen .shopp_items .shopp_item .items_cen .cenitem  .lefts .icon_none{
   margin-bottom: 2rpx;
  vertical-align: top
} 
.cenitemnew{
  display:flex;
  display:-webkit-flex;
  justify-content:flex-start;
  align-items:center;
  width: 100%;
  padding-left: 20rpx
}
.cenitemnew .cenitem_img{
  width: 60px;
  height: 60px;
  display: inline-block;
}
 .cenitemnew .cenitem_img image{
  width: 60px;
  height: 60px;
}
.cenitemnew .itemnew{
  width: 100%;
  display: inline-block;
  margin-left: 10px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.itemnew .new_name{
  width: 215px;
  font-size: 15px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 15px;
}
.itemnew .new_pri{
  font-size: 12px;
  position: relative;
}
.itemnew .new_num{
  color: #999;
}
.itemnew .new_money{
  color: #666;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 15px;
}
.shopping .shopp_cen .shopp_items .shopp_item .items_cen .Packing{
  font-size: 12px;
  position: relative;
  width: 100%;
  padding: 10px 10px 10px 30px;
  box-sizing: border-box;
}
.shopping .shopp_cen .shopp_items .shopp_item .items_cen .Packing .packing_font{
  color: #666
}
.shopping .shopp_cen .shopp_items .shopp_item .items_cen .Packing .pack_price{
  color: #666;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 5px;
}
.shopping .shopp_cen .shopp_items .shopp_item .Settlement{
  width: 100%;
  padding: 10px 10px 0px 10px;
  box-sizing: border-box;
  text-align: right;
  position: relative;
}
.shopping .shopp_cen .shopp_items .shopp_item .Settlement .youhui{
  position: absolute;
  top: 12px;
  left: 10px;
  color: #333;
  font-size: 14px;
}
.shopping .shopp_cen .shopp_items .shopp_item .Settlement .settle_price{
  display: inline-block;
  font-size: 15px;
  color: #333;
}
.shopping .shopp_cen .shopp_items .shopp_item .Settlement .settle_sub{
  display: inline-block;
  margin-left: 10px;
  font-size: 13px;
  padding: 3px 10px;
  color: #fff;
  background: #f7636d;
}


