

.home{
  width: 100%;
  height: 100%;
}
.navigator-hover {
	background-color: #fff;
}
/*定位位置*/
.home>.title{
  display: flex;
  background-color: #fff;
  position: fixed;
  top: 0;
  z-index: 999;
  width: 100%;
}
.home .conta_title{
  /* width: 100%; */
  height: 40px;
  line-height: 40px;
  background: #fff;
  padding: 0px 15px;
  box-sizing: border-box;
  position: relative;
  -webkit-transition: all 5s;
}
.jiedao{
  display:inline-block;
   font-size: 30rpx;
  color: #333;
}
.jiedaoimg{
  display:inline-block;
}
.jiedaoimg>image{
  width: 20rpx;
  height: 10rpx;
  margin-left: 5px;
  vertical-align: middle
}
.home .conta_title .shopp{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 15px;
}
.home .conta_title .shopp image{
  width: 100%;
  height: 100%;
}

/*搜索框*/
.home .home_search{
  /* width: 100%; */
  /* display: inline-block; */
   /* position: fixed; */
	/* z-index: 10;
	top: 40px;
	left: 0; */
  position: relative;
  flex: 1;
  padding: 5px 15px;
