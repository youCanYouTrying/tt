}

.home .home_search>.simage{
   width:42rpx;
  height:42rpx;
  position:absolute;
  top:calc(50% - 24rpx);
  background-size:100%;
  left:52rpx;
}
.home .home_con{
  width: 100%;
  height: 100%;
  -webkit-transition: all 0.5s;
  margin-top: 90rpx
}

/*banner部分*/
.scroll-view{
  height: 100%;
}
.banner {
	height: 330rpx;
  margin-top: 0px;
}
.banner image {
	width: 100%;
	height: 100%;
}
/*滑动小圆点样式*/
.banner .wx-swiper-dots.wx-swiper-dots-horizontal{
     margin-bottom: 2rpx;
}
.banner .wx-swiper-dot{
    width:10rpx;
    display: inline-flex;
    height: 10rpx;
    margin-left: 15rpx;
    justify-content:space-between;
}
.banner .wx-swiper-dot::before{
    content: '';
    flex-grow: 1; 
    background: #dbdbdb;
    border-radius: 50%;
}
.banner .wx-swiper-dot-active::before{
    background:#fff;   
}
/*分类*/
.icons {
	height: 330rpx;
	padding: 20rpx 10rpx;
	padding-bottom: 0;
  background: #fff;
}
.icons swiper-item {
	width: 100%;
	/*display: -webkit-flex;*/
	/*flex-direction: row;*/
	/*flex-wrap: wrap;*/
	/*justify-content: center;*/
}
.icons .icon {
	float: left;
	display: -webkit-flex;
	flex-direction: column;
	flex-wrap: wrap;
	width: 20%;
	text-align: center;
	margin: 10rpx 0;
}
.icons .icon image {
	margin: 0 auto;
	width: 80rpx;
	height: 80rpx;
}
.icons .icon text {
	color: #000;
	font-size: 24rpx;
	line-height: 2;
}
/*滑动小圆点样式*/
.icons .wx-swiper-dots .wx-swiper-dots-horizontal{
     margin-bottom: 2rpx;
}
.icons .wx-swiper-dot{
    width:35rpx;
    display: inline-flex;
    height: 8rpx;
    margin-left: 15rpx;
    justify-content:space-between;
}
.icons .wx-swiper-dot::before{
    content: '';
    flex-grow: 1; 
    background: #dbdbdb;
    border-radius: 10rpx
}
.icons .wx-swiper-dot-active::before{
    background:#fc6f6a;   
}

/*分类板块*/
.section-big {
	padding: 20rpx;
}
.section-big .title {
  height: 60rpx;
	position: relative;
	text-align: center;
	font-size: 30rpx;
	font-weight: bold;
	line-height: 60rpx;
	margin-bottom: 20rpx;
  z-index: 1;
  color: #333;

}
.title1{
  display: flex;
  height: 60rpx;
  line-height: 1;
	position: relative;
  justify-content:center;
  align-items: center;
  font-size: 30rpx;
	font-weight: bold;
  color: #333;
  margin-bottom: 20rpx;
}
.title1>span{
  display: inline-block;
  border-radius: 50%;
  width: 6rpx;
  height: 6rpx;
  background-color: #333;
  position: relative
}
.title1>.title-left{
 margin-right: 20rpx;
}
.title1>.title-right{
 margin-left: 20rpx;
}
.title1:before {
	content: "";
	position: absolute;
	left: 50%;
	top: 50%;
	width: 70rpx;
	height: 1rpx;
  box-sizing: border-box;
	background-color: #333;
	margin-left: -150rpx;
}
.title1:after{
	content: " ";
	position: absolute;
	left: 50%;
	top: 50%;
	margin-left: 80rpx;
	width: 70rpx;
	height: 1rpx;
	background: #333;
}

