.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9;
  color: #fff;
}
.modal-dialog {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 40px;
  left: 0;
  z-index: 11;
  background: #fff;
}
.modal-content {
  padding: 50rpx 32rpx;
  padding-top: 0;
  padding-bottom: 0px
}
.modal-item{
  font-size: 28rpx;
  height: 100rpx;
}
.modal-item>view{
  width: 100%;
  height: 100%;
  line-height: 100rpx;
  border-top: 1rpx solid #e5e5e5
}
.modal-list{
   font-size:14px;
}
.modal-list>view{
  height: 40px;
  border-top:1px solid #eee;
  line-height: 40px;
  padding-left: 20px
}
.modal-list>view:last-child{
 border-bottom: 1px solid #eee;
}
