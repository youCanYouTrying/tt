.container{
  width: 100%;
  height:100%;
}
.content{
  background: #fff;
  width: 100%;
  font-size: 30rpx;
}
.item{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  padding:0 0 0 30rpx;
   overflow: hiddenï¼›
}
.item-left{
  flex: none;
  width: 120rpx;
  padding-left: 30rpx;
}
.item-right{
  flex:auto;
  padding:30rpx 0;
  text-align: left;
}
.bor{
  border-bottom:1px solid #e5e5e5; 
}
.icon_none{
  width:40rpx;
  height:40rpx;
  border:1rpx solid #d6d6d6;
  border-radius:50%;
}
.radio{
  display: inline-block;
  margin-right: 100rpx; 
}
.radio icon,.radio text{
    vertical-align: middle
}
.radio icon{
  margin-right: 20rpx;
}
.btn{
  margin: 0 auto;
  margin-top: 50rpx;
  width: 92%;
	height: 82rpx;
	background-color: #fe6e78;
	border-radius: 2px;
  text-align: center;
  color: #fff;
  line-height: 82rpx;
}
.tishi{
  color: #999;
  font-size: 24rpx;
  margin-top: 10rpx;
  text-align: center
}
