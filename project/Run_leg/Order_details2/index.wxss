}
.container .receipt{
  width: 100%;
  position: absolute;
  top: 0px;
  left: 0px;
  z-index: 1;
  padding: 0px 10px 0px 10px;
  box-sizing: border-box;
}
.container .receipt .close_rece{
  width: 100%;
  height: 40px;
}
.container .receipt .close_rece .close_img{
  position: absolute;
  top: 10px;
  left: 20px;
  width: 35px;
  height: 35px;
}
.container .receipt .close_rece .close_img image{
  width: 35px;
  height: 35px;
}
.container .receipt .close_rece .fenx_img{
  position: absolute;
  top: 10px;
  right: 20px;
   width: 35px;
  height: 35px;
}
.container .receipt .close_rece .fenx_img image{
  width: 35px;
  height: 35px;
}
/*商家位置*/
.container  .map_dian{
  position: absolute;
  top: 150px;
  left: 50%;
  transform: translateX(-50%);
  text-align: center;
}
.container  .buyer{
  position: absolute;
  top: 100px;
  right: 40px;
}
.container  .buyer i{
  width: 50px;
  height: 50px;
}
.container  .buyer i image{
  width: 50px;
  height: 50px;
}
.container  .map_dian span{
  display: block;
  width: 100%;
  text-align: center;
  font-size: 14px;
  background: white;
  padding: 5px 7px;
  border-radius: 6px;
}
.container  .map_dian i{
  width: 50px;
  height: 50px;
  margin: auto;
}
.container  .map_dian i image{
  width: 50px;
  height: 60px;
}
/*取消订单*/
.container .receipt .receipt_con{
  width: 100%;
  position: absolute;
  top: 350px;
  padding: 0px 10px 10px 0px;
  box-sizing: border-box;
  margin-bottom: 10px;
}
.container .receipt .close_reces{
     width: 100%;
}
.container .receipt .close_reces .reces{
  background: white;
  margin-right: 10px;
  padding-top: 40px;
  padding-bottom: 15px;
  box-sizing: border-box;
  text-align: center;
}
.container .receipt .close_reces .reces p{
    margin-bottom: 10px;
    font-weight: bold;
    width: 100%;
    text-align: center;
    display: block;
}
.container .receipt .close_reces .reces p span{
  margin-bottom: 10px;
  font-size: 14px;
  color: #666666;
  font-weight: normal;
  position: relative;
  padding-left: 25px;
  box-sizing: border-box;
}
.container .receipt .close_reces .reces p span i{
  width: 18px;
  height: 18px;
  position: absolute;
  top: 1px;
  left: 5px;
}
.container .receipt .close_reces .reces p span i image{
  width: 18px;
  height: 18px;
}
.container .receipt .close_reces .reces p b{
  font-weight: normal;
  font-size: 14px;
  color: #fe7272;
}
.container .receipt .close_reces .reces .rece_close{
      width: 100%;
      text-align: center;
      margin-bottom: 10px;
}
.container .receipt .close_reces .reces .rece_close span{
 width: 80px;
 height: 35px;
 margin: auto 5px;
  border: 1px solid #bfbfbf;
  border-radius: 3px;
  font-size: 15px;
  text-align: center;
  line-height: 35px;
  display: inline-block;
}
.container .receipt .close_reces .reces .rece_close #cui{
  background: #f5f5f5;
  color: #ccc;
}
.container .receipt .close_reces .reces .rece_close #submit{
  background: #fc656f;
  color: #fff;
  border: 1px solid #fc656f;
}
.container .receipt .receipt_cen{
  width: 100%;
  margin-top: 10px;
}
.container .receipt .receipt_cen .receipt_items{
  background: white;
  margin-right: 10px;
  margin-bottom: 10px;
  border-radius: 7px;
}
.container .receipt .receipt_cen .receipt_items .Rider{
   width: 100%;
   position: relative;
}
.container .receipt .receipt_cen .receipt_items .Rider .rider_con{
