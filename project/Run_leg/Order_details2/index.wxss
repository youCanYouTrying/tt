  position: absolute;
  left: 10px;
  top: 15px;
}
.container .receipt .receipt_cen .receipt_items .Rider .rider_con #rider_img image{
   width: 50px;
  height: 50px;
}
.container .receipt .receipt_cen .receipt_items .Rider .rider_con p{
  display: block;
