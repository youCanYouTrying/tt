.container .receipt .receipt_cen .receipt_items .receipt_item span{
  line-height: 45px;
  margin-left: 5px;
  font-size: 14px;
  display: inline-block;
  color: #999;
  max-width: 200px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.container .receipt .receipt_cen .receipt_items .receipt_item #font1{
  color: #333;
}
.container .receipt .receipt_cen .receipt_items .receipt_item .font{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 5px;
  color: #666666;
  font-size: 14px;
  max-width: 175px;
   display: -webkit-box ;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -moz-box-orient: vertical;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2; 
  -moz-line-clamp:2; 
}
.container .receipt .receipt_cen .receipt_items .item4 span{
  color: #333;
}
.container .receipt .receipt_cen .receipt_items .item4 .font{
