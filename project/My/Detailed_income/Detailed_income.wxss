.container{
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  padding: 0rpx 30rpx;
  box-sizing: border-box;
  border-top: 2rpx solid#ddd;
  }

.income_number{
  width: 100%;
  height: 210rpx;
  border-bottom: 2rpx solid#ddd;
  display: flex;
  vertical-align: center;
}
.income_number .number_title{
  width: 50%;
  height: 210rpx;
  line-height: 210rpx;
  font-size: 28rpx;
}
.income_number .number{
  width: 50%;
  height: 210rpx;
  line-height: 210rpx;
  text-align: right;
  color: #ff7d3a;
  font-size: 60rpx;
}
.income_list{
  display: flex;
  vertical-align: center;
  height:100rpx;
  font-size: 28rpx;
}
.list_title{
   width: 50%;
   height:100rpx;
   line-height: 100rpx;
   color:#999;
}
.list_content{
  width: 50%;
  text-align: right;
  height:100rpx;
   line-height: 100rpx;
}
