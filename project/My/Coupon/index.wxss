/* Run_leg/run_leg/index.wxss */
.container {
  height: 100%;
} 

page{
  height: 100%;
}

.coupon{
  width: 100%;
  height: 100%;
}
.coupon  .top-table{
  width: 100%;
  background: white;
  text-align: center;
  padding: 20rpx 0rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #e6e6e6;
 
}
.coupon  .top-table .scroll-view_table{
  width: 280rpx;
  border: 2rpx solid #fc656f;
  margin: auto;
  border-radius: 10rpx;
}
.coupon  .top-table .table-coll{
  display: inline-block;
  width: 140rpx;
  height: 100%;
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #fc656f;
}
.coupon  .top-table .selected{
  color: #fff;
  background: #fc656f;
}

/*搜索部分*/
.coupon .search{
  margin:20rpx 0rpx;
  width: 100%;
  height: 80rpx;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
  position: relative;
}
.coupon .search .search_img{
    position: absolute;
    width: 50rpx;
    height: 50rpx;
    top: 50%;
    left: 20rpx;
    transform: translateY(-50%);
    z-index: 4;
}
.coupon .search .search_img image{
  position: absolute;
    width: 50rpx;
    height: 50rpx;
    top: 50%;
    left: 20rpx;
    transform: translateY(-50%);
    z-index: 4;
}
.coupon .search .input{
  width: 100%;
  height: 40px;
  background: white;
  border: 2rpx solid #ebebeb;
  border-radius:10rpx;
  padding-left: 90rpx;
  box-sizing: border-box;
  font-size: 28rpx;
}
/*内容部分1*/
.coupon .table-swiper{
  width: 100%;
  padding: 0rpx 20rpx;
  display: block; 
  height: 100%;
  box-sizing: border-box; 
  overflow: hidden;
}
.coupon .table-swiper .soupon_item{
  width: 100%;
  height: 200rpx;
  position: relative;
  padding: 10rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}
.coupon .table-swiper .soupon_item:last-child{
  margin-bottom: 200rpx;
}
 .coupon_bg{
  width: 100%;
  height: 200rpx;
  /* position: absolute;
  top: 0rpx;
  left: 0rpx; */
  /* z-index: 1; */
}
 .coupon_con{
  width: 100%;
  height: 100px;
  background-repeat:no-repeat;
  background-size: 100%;
  display: flex;
  align-items: center;
  background-position-y:4rpx;
}
 .coupon_con .coupon_left{
  flex: none;
  width: 138rpx;
  box-sizing: border-box;
  padding-left: 31rpx;
}
.coupon_con>.center{
  padding-left: 24rpx;
  flex: 1;
}
.center>.item_name{
  width: 100%;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
  color: #333;
}
 .center>.coupon_juan{
  width: 100%;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
  color: #333;
  margin:10rpx 0rpx;

}
.center>.coupon_time{
  width: 100%;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
  color: #666;
  margin: 10rpx 0rpx;
}
.center>.coupon_juan span{
  padding: 4rpx 14rpx;
  box-sizing: border-box;
  display: inline-block;
  border: 2rpx solid #fe6161;
  font-size: 24rpx;
  color: #fe6161;
}
.coupon_con .coupon_left .item_img{
  width: 120rpx;
  height:120rpx;
  border-radius: 20rpx;
  /* position: absolute;
  left: 20rpx;
  top: 20rpx; */
}
.coupon_con .coupon_left .item_img image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 20rpx;
}
/*右边部分*/
.coupon_con .coupon_right{
  width: 204rpx;
  height: 160rpx;
  border-left: 2rpx solid #dbdbdb;
  box-sizing:border-box;
}
.coupon_con .coupon_right .right_price{
  width: 100%;
  text-align: center;
  font-size: 54rpx;
  color: #fd6767;
}
 .coupon_con .coupon_right .right_price span{
  font-size: 40rpx;
}
 .coupon_con .coupon_right .right_remak{
  width: 100%;
  font-size: 26rpx;
  text-align: center;
  margin-top: 14rpx;
  color: #666
}
.no_more{
  color: #333;
}
/* 红包 */
.t_left{
  width: 30%;
}
.t_left .reduce_money{
  font-size: 60rpx;
  color:#ff001d;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.redu_icon{
  font-weight:400;
  font-size: 45rpx;
}
.redu_condition{
  color:#666666;
  font-size:28rpx;
}
.t_center{
  width: 40%;
}
.t_center > view{
  width: 100%;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.t-right{
  width: 30%;
  height:140rpx;
  border-left:2rpx solid #dbdbdb;
}
.t_center .redu_title{
  font-size: 32rpx;
  font-weight: bold;
  color: rgb(51,51,51);
}
.t_center .redu_time{
  margin-top: 4rpx;
  font-size: 24rpx;
  color:#666666;
}

.t-right .redu_btn{
  border:2rpx solid rgb(255,84,96);
  border-radius:6rpx;
  padding:6rpx 20rpx;
  box-sizing:border-box;
  color:rgb(255,84,96);
  font-size:30rpx;
  width:160rpx;
  text-align:center;
  margin:40rpx 16rpx;
}
.t-right .redu_btn2{
  border:2rpx solid rgb(255,84,96);
  border-radius:6rpx;
  padding:6rpx 10rpx;
  box-sizing:border-box;
  color:#fff;
  font-size:30rpx;
  width:160rpx;
  text-align:center;
  margin:40rpx 20rpx;
  background-color: rgb(255,84,96);
}
scroll-view {
  height: 100%;
}
