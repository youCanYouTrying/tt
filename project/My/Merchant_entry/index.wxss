page{
  border-top: 1px solid #e6e6e6;
  background: #f5f5f5;
}
.entry{
  width: 100%;
  height: 100%;
  position: relative;
}
.entry .top-table{
  width: 100%;
  height: 45px;
  line-height: 45px;
  position: fixed;
  top: 0px;
  left: 0px;
  z-index: 6;
}
.entry .top-table .scroll-view_table{
  width: 100%;
  height: 45px;
  background: white;
}
.entry .top-table .scroll-view_table .table-coll{
  display: inline-block;
  width: 25%;
  height: 45px;
  text-align: center;
  font-size: 14px;
  color: #666666;
  position: relative;
}
.entry .top-table .scroll-view_table .table-coll span{
  display: none;
  /* display: inline-block; */
  width: 18px;
  height: 2px;
  background: #f95e8c;
  position: absolute;
  bottom: 4px;
  left: 50%;
  transform: translateX(-50%);
}
.entry .top-table .scroll-view_table .selected span{
  display: inline-block;

}
.entry .top-table .scroll-view_table .selected {
  color: #333333;
}
.entry .table-swiper{
  width: 100%;
  padding: 20px 20px 0px 20px;
  height: 100%;
  box-sizing: border-box;
  margin-top: 45px;
}
.entry .table-swiper .process{
  width: 100%;
}
.entry .table-swiper .process .process_item{
  width: 100%;
  margin-bottom: 15px;
}
.entry .table-swiper .process .process_item .process_name{
  font-size: 16px;
  color: #333;
  margin-bottom: 4px;
}
.entry .table-swiper .process .process_item .procss_con{
  width: 100%;
  font-size: 14px;
  color: #666;
  margin: 3px 0px;
}
.entry .xiazai{
  width: 100%;
  height: 55px;
  position: fixed;
  bottom: 0px;
  left: 0px;
  padding: 5px 10px;
  box-sizing: border-box;
}
.entry .xiazai .sub{
  width: 100%;
  height: 45px;
  background: #ff424f;
  border-radius: 5px;
  text-align: center;
  line-height: 45px;
  color: white;
}