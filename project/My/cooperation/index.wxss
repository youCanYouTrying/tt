.cooperation{
  width: 100%;
  padding-top: 10px;
  box-sizing: border-box;
  height: 100%;
  overflow: hidden;
}
.cooperation .Admission{
  width: 100%;
  height: 50px;
  background: white;
  position: relative;
  margin-bottom: 10px;
}
.cooperation .Admission .admisson{
  position: absolute;
  left: 10px;
  top: 50%;
  transform: translateY(-50%);
  padding-left: 60px;
  box-sizing: border-box;
}
.cooperation .Admission .admisson .admiss_img{
  width: 40px;
  height: 40px;
  border-radius: 50%;
  background: #ff8e61;
  position: absolute;
  left: 10px;
  top: 50%;
  transform: translateY(-50%);
}
.cooperation .Admission .admisson .admiss_img image{
  width: 20px;
  height: 20px;
  position: absolute;
  left: 10px;
  top: 50%;
  transform: translateY(-50%);
}
.cooperation .Admission .Shop{
  position: absolute;
  top: 50%;
  right: 10px;
  transform: translateY(-50%);
  display: inline-block;
  padding: 3px 10px;
  border: 1px solid #ff424f;
  color: #ff424f;
  font-size: 12px;
}

/*城市代理*/
.cooperation .agent{
  width: 100%;
  height: 50px;
  background: white;
  position: relative;
  margin-bottom: 10px;
}
.cooperation .agent .agents{
  position: absolute;
  left: 10px;
  top: 50%;
  transform: translateY(-50%);
  padding-left: 60px;
  box-sizing: border-box;
}
.cooperation .agent .agents .agent_img{
  width: 40px;
  height: 40px;
  border-radius: 50%;
  background: #6eafff;
  position: absolute;
  left: 10px;
  top: 50%;
  transform: translateY(-50%);
}
.cooperation .agent .agents .agent_img image{
  width: 25px;
  height: 25px;
  position: absolute;
  left: 8px;
  top: 50%;
  transform: translateY(-50%);
}
.cooperation .agent .Shop{
  position: absolute;
  top: 50%;
  right: 10px;
  transform: translateY(-50%);
  display: inline-block;
  padding: 3px 10px;
  border: 1px solid #ff424f;
  color: #ff424f;
  font-size: 12px;
}

/*分享*/
.cooperation .share{
  width: 100%;
  position: fixed;
  bottom: 0px;
  left: 0px;
  height: 45px;
  background: #fff;
  border-top: 1px solid #e6e6e6;
  text-align: center;
  line-height: 45px;
  color: #333;
}