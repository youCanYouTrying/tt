/* My/Member/index.wxss */
.member{
  width: 100%;
  background: white;
  height: 100%;
}
.member .member_bg{
  width: 100%;
  height: 150px;
  overflow: hidden;
  padding: 2px;
  box-sizing: border-box;
  position: relative;
  background: white;
}
.member .member_bg image{
  width: 100%;
  height: 150px;
}
.member .member_bg .member_title{
  width: 100%;
  height: 150px;
  z-index: 3;
  position: absolute;
  top: 0px;
  left: 0px;
}
.member .member_bg .member_title .mem_quan{
  position: absolute;
  top: 10px;
  right: 10px;
  padding-right: 25px;
  box-sizing: border-box;
  font-size: 13px;
  color: white;
}
.member .member_bg .member_title .mem_quan .mem_image{
  width: 10px;
  height: 10px;
  
}
.member .member_bg .member_title .mem_quan .mem_image image{
  width: 8px;
  height: 10px;
  position: absolute;
  top: 4px;
  right: 10px;
}
.member .member_bg .member_title .vip{
  position: absolute;
  bottom: 20px;
  left: 10px;
  width: 100%;
}
.member .member_bg .member_title .vip .vip_left{
  position: absolute;
  bottom: 0px;
  left: 10px;
   width: 70px;
  height: 40px;
}
.member .member_bg .member_title .vip .vip_left image{
  width: 70px;
  height: 40px;
}
.member .member_bg .member_title .vip .vip_right{
  padding-left:85px;
  box-sizing: border-box;
}
.member .member_bg .member_title .vip .vip_right .vip_font{
  width: 100%;
  font-size: 18px;
  color: #fff;
}
.member .member_bg .member_title .vip .vip_right .vip_text{
  width: 100%;
  margin-top: 5px;
}
.member .member_bg .member_title .vip .vip_right .vip_text .text1{
  float: left;
  font-size: 13px;
  color: #fff;
}
.member .member_bg .member_title .vip .vip_right .vip_text .text2{
  float: left;
  font-size: 13px;
  color: #fff;
}
.member .member_bg .member_title .vip .vip_right .vip_text .vip_img{
  width: 10px;
  height: 10px;
  float: left;
  margin: 0px 5px;
}
.member .member_bg .member_title .vip .vip_right .vip_text .vip_img image{
  width: 10px;
  height: 10px;
}



/*内容部分*/
.mem_content{
  width: 100%;
  padding: 5px 10px 10px 10px;
  box-sizing: border-box;
  background: white;
}
.mem_content .mem_item{
  width: 100%;
  height: 90px;
  background: white;
  position: relative;
  box-shadow: 1px 5px 20px #e6e6e6;
  margin-bottom: 20px;
  border-radius: 5px;
}
.mem_content .mem_item .mem_shop{
  width:80px;
  height: 30px;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 10px;
  background: #ed8093;
  text-align: center;
  line-height: 30px;
  font-size: 14px;
  color: white;
  border-radius: 50px;
}
.mem_content .mem_item .mem_time{
  position: absolute;
  top: 15px;
  left: 10px;
}
.mem_content .mem_item .mem_time .mem_tim{
  width: 100%;
  font-size: 15px;
  color: #333;
}
.mem_content .mem_item .mem_time .mem_tim .mem_price{
  margin-left: 15px;
  color: #ff5757;
}
.mem_content .mem_item .mem_time .mem_num{
  margin-top: 20px;
  font-size: 13px;
  color: #666;
}