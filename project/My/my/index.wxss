.my{
  width: 100%;
}
/*第一部分*/
.my  .my_title{
  width: 100%;
  height: 300rpx;
  overflow: hidden;
  position: relative;
}
.my  .my_title image{
  width: 100%;
  height: 100%;
}
.my  .my_title .title_con{
  width: 100%;
  height: 300rpx;
  position: absolute;
  top: 0px;
  left: 0px;
  z-index: 2;
}
.my  .my_title .title_con .Set{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 30rpx;
  left: 30rpx;
}
.my  .my_title .title_con .Set image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 0px;
  left: 0px;
} 
.my  .my_title .title_con .Prompt{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 30rpx;
  right: 30rpx;
}
.my  .my_title .title_con .Prompt image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 0px;
  right: 0px;
} 
.my  .my_title .title_con .my_name{
  /* height: 140rpx; */
  position: absolute;
  bottom: 40rpx;
  left: 50%;
  transform: translateX(-50%);
}
.my  .my_title .title_con .my_name .name_img{
  width: 100rpx;
  height: 100rpx;
  margin: auto;
  border-radius: 50%;
} 
.my  .my_title .title_con .my_name .name_img image{
  width: 100rpx;
  height: 100rpx;
  margin: auto;
  border-radius: 50%;
}
.my  .my_title .title_con .my_name .name{
  width: 400rpx;
}
.my  .my_title .title_con .my_name .name .sign{
  width: 100%;
  display: inline-block;
  text-align: center;
  font-size: 30rpx;
  color: white;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/*第二部分*/
.my .my_con1{
  width: 100%;
  background: white;
  padding: 40rpx 40rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}
.my .my_con1 .con1_item{
  display: inline-block;
  margin-right: 80rpx;
}
.my .my_con1 .con1_item .item_img{
  width: 80rpx;
  height: 70rpx;
  margin: auto;
}
.my .my_con1 .con1_item .item_img image{
  width: 80rpx;
  height: 68rpx;
  margin: auto;
}
.my .my_con1 .con1_item .item_font{
  width: 100%;
  text-align: center;
  margin-top: 10rpx;
  font-size: 30rpx;
}

/*第三部分*/
.my .my_con2{
  width: 100%;
  background: white;
  padding: 0rpx 30rpx;
  box-sizing: border-box;
  margin-bottom: 60rpx;
}
.my .my_con2 .used:first-of-type{
    border-bottom: 2rpx solid #ebebeb;
}
.my .my_con2 .used .used_font{
  width: 100%;
  font-size: 38rpx;
  font-weight: bold;
}
.my .my_con2 .used{
  padding: 10px 0px;
  box-sizing: border-box;
}
.my .my_con2 .used .used_items{
  width: 100%;
  padding: 30rpx 0px 0rpx 0rpx;
  box-sizing:border-box;
}
.my>.my_con2>.used>.used_items>.used_item{
  width: 120rpx;
  display: inline-block;
  margin-right: 66rpx;
}
button::after{
border:none;
}
input{
outline:none;
border:none;
list-style: none;
}
.shar_btn{
  padding: 0;
  line-height: 1;
  background-color: #fff;
  color: #333;
  overflow:no;
  position: no;
}
.my>.my_con2>.used>.used_items>.used_item:last-child{
  margin-right: 0rpx;
}
.my .my_con2 .used .used_items .used_item .used_img{
  width: 60rpx;
  height: 60rpx;
  margin: auto;
}
.my .my_con2 .used .used_items .used_item .used_img image{
  width: 60rpx;
  height: 60rpx;
  margin: auto;
}
.my .my_con2 .used .used_items .used_item .used_name{
  width: 100%;
  margin-top: 8rpx;
  text-align: center;
  font-size: 26rpx;
}
.notice_num{
  position: absolute;
  top: 0;
  left:0;
  color: #ffffff;
  width: 30rpx;
  height: 30rpx;
  font-size: 20rpx;
  text-align: center;
  line-height: 30rpx;
  background-color: red;
  /* border: 2rpx solid#ffffff; */
  border-radius: 50%;
  margin-left: 20rpx;
}