page{
  width: 100%;
  height: 100%;
  background: white;
  border-top: 2rpx solid #f5f5f5;
  overflow: hidden;
}
.sign_in{
  width: 100%;
  padding: 40rpx 20rpx 40rpx 20rpx;
  box-sizing: border-box;
  height: 100%;
  overflow: hidden;
}
.sign_in .form_sub{
  width: 100%;
}
.sign_in .form_sub .phone{
  width: 100%;
  height: 90rpx;
  border-bottom: 2rpx solid #dbdbdb;
  position: relative;
}
.sign_in .form_sub .phone .input_phone{
  width: 100%;
  display: inline-block;
  height: 90rpx;
  font-size: 28rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.sign_in .form_sub .pwd{
  width: 100%;
  height: 90rpx;
  border-bottom: 2rpx solid #dbdbdb;
  position: relative;
  margin-top: 40rpx;
} 
.sign_in .form_sub .pwd .input_pwd{
  width: 60%;
  display: inline-block;
  height: 90rpx;
  font-size: 28rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.sign_in .form_sub .pwd .yanz{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  display: inline-block;
  right: 10rpx;
  font-size: 28rpx;
  color: #1a92ee;
}
.sign_in .form_sub .register{
  width: 100%;
  height: 90rpx;
  position: relative;
  margin-top: 30rpx;
}
.sign_in .form_sub .register .reg_font{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 10rpx;
  font-size: 28rpx;
  color: #999;
}
.sign_in .form_sub .register .reg_gis{
  position: absolute;
  top: 50%;
  right: 20rpx;
  transform: translateY(-50%);
  font-size: 28rpx;
  color: #999;
}
.sign_in .form_sub  .submit{
  width: 100%;
  height: 80rpx;
  position: relative;
  margin-top: 30rpx;
}
.sign_in .form_sub  .submit .submits{
    display: inline-block;
    width: 100%;
    height: 90rpx;
    background: #ebebeb;
    text-align: center;
    line-height: 90rpx;
    color: #b3b3b3;
     border-radius: 0;
     border: 0;
     font-size: 30rpx;
}
button::after{
    border:none;
}
.sign_in .form_sub .Other{
  margin-top: 30rpx;
  text-align: right;
  font-size: 28rpx;
  color: #f7636d;
}
.sign_in .Agreement{
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
  color: #666;
  width: 100%;
  height: 40px;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
  text-align: center;
}
.sign_in .Agreement span{
  display: inline-block;
  font-size: 28rpx;
}
.sign_in .Agreement span .agree{
  color: #f7636d;
  text-decoration: underline;
}
.sign_in .weui-toptips{
  width: 100%;
  height: 80rpx;
  text-align: center;
  background: #f5f5f5;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #fd6767;
}
.delet_all{
  width: 20rpx;
  height: 20rpx;
  top: 0;
  right: 0;
  margin-top: 40rpx;
  margin-right: 40rpx;
  position: absolute;
  z-index: 11;
}
.see{
  width: 40rpx;
  height: 24rpx;
  position: absolute;
  right: 0;
  top:0;
  margin-top: 40rpx;
  margin-right: 80rpx; 
  z-index: 10;
  }